    "dotenv": "^16.3.1",
    "hono": "^3.12.3",
    "isbot": "^3.6.8",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
